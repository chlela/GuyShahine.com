<!DOCTYPE html>
<html>
<head prefix="og: http://ogp.me/ns#">
  <meta charset="utf-8" />
  <meta name="viewport" content="width=device-width, initial-scale=1.0" />
  <title>@ViewData["Title"]</title>

  <environment names="Development">
    <link rel="stylesheet" href="~/lib/semantic/dist/semantic.css" />
    <link rel="stylesheet" href="~/css/site.css" />
  </environment>
  <environment names="Staging,Production">
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/semantic-ui/2.2.4/semantic.min.css" asp-fallback-href="~/lib/semantic/dist/semantic.min.css" asp-fallback-test-class="sr-only" asp-fallback-test-property="position" asp-fallback-test-value="absolute" />

    <link rel="stylesheet" href="~/css/site.min.css" asp-append-version="true" />

    <script>
      // Google Analytics
      (function (i, s, o, g, r, a, m) {
        i['GoogleAnalyticsObject'] = r; i[r] = i[r] || function () {
          (i[r].q = i[r].q || []).push(arguments)
        }, i[r].l = 1 * new Date(); a = s.createElement(o),
        m = s.getElementsByTagName(o)[0]; a.async = 1; a.src = g; m.parentNode.insertBefore(a, m)
      })(window, document, 'script', '//www.google-analytics.com/analytics.js', 'ga');

      ga('create', 'UA-15230844-1', 'auto');
      ga('send', 'pageview');
    </script>
  </environment>

  @RenderSection("head", required: false)
</head>
<body>
  <div class="ui visible left vertical inverted sidebar labeled icon menu">
    <a class="item" href="/guestlist">
      <i class="users icon"></i>
      All
    </a>

    <a class="item" href="/guestlist/guestOf/Soutou">
      <i class="male icon"></i>
      Soutou
    </a>

    <a class="item" href="/guestlist/guestOf/Shahine">
      <i class="female icon"></i>
      Shahine
    </a>

    <a class="item" href="/guestlist/guestOf/Rachid">
      <i class="man icon"></i>
      Rachid
    </a>

    <a class="item" href="/guestlist/guestOf/Jamie">
      <i class="woman icon"></i>
      Jamie
    </a>

    <a class="item" href="/guestlist/guestOf/Joud">
      <i class="diamond icon"></i>
      Joud
    </a>

    <a class="item" href="/guestlist/guestOf/Mct">
      <i class="smile icon"></i>
      Mct
    </a>
  </div>

  @RenderBody()

  <environment names="Development">
    <script src="~/lib/jquery/dist/jquery.js"></script>
    <script src="~/lib/semantic/dist/semantic.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
  </environment>
  <environment names="Staging,Production">
    <script src="https://ajax.aspnetcdn.com/ajax/jquery/jquery-2.2.0.min.js"
            asp-fallback-src="~/lib/jquery/dist/jquery.min.js"
            asp-fallback-test="window.jQuery">
    </script>

    <script src="https://cdn.jsdelivr.net/semantic-ui/2.2.4/semantic.min.js" asp-fallback-src="~/lib/semantic/dist/semantic.min.js" asp-fallback-test="window.jQuery && window.jQuery.fn && window.jQuery.fn.modal">
    </script>

    <script src="~/js/site.min.js" asp-append-version="true"></script>
  </environment>

  @RenderSection("scripts", required: false)
</body>
</html>