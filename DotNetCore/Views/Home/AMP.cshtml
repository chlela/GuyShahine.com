  <script async custom-element="amp-carousel" src="https://cdn.ampproject.org/v0/amp-carousel-0.1.js"></script>
  <script async custom-element="amp-form" src="https://cdn.ampproject.org/v0/amp-form-0.1.js"></script>

  <script type="application/ld+json">
    {
    "@@context": "http://schema.org/",
    "@@type": "Person",
    "name": "Guy Shahine",
    "url": "http://gshahine.com",
    "sameAs": [
    "https://www.facebook.com/gshahine",
    "https://instagram.com/mrchlela",
    "https://twitter.com/mrchlela",
    "https://www.linkedin.com/in/gshahine",
    "https://plus.google.com/+GuyShahine"
    ],
    "givenName": "Guy",
    "familyName": "Shahine",
    "image": "http://gshahine.com/images/guy-shahine.jpg",
    "jobTitle": "Software Engineer",
    "contactPoint": {
    "@@type": "ContactPoint",
    "email": "hey@gshahine.com",
    "telephone": "+14242343361",
    "contactType": "technical support",
    "areaServed": "US",
    "availableLanguage": ["English", "French", "Arabic"]
    },
    "homeLocation": {
    "@@type": "PostalAddress",
    "addressLocality": "San Francisco",
    "addressRegion": "CA",
    "addressCountry": "USA"
    }
    }
  </script>

  <style amp-custom>
    body {
      font-family: Lato,'Helvetica Neue',Arial,Helvetica,sans-serif;
    }

    a {
      color: #70c1b3;
      text-decoration: none;
    }

    .container {
      box-sizing: border-box;
      margin: 0 auto;
      max-width: 1000px;
      padding: 0 20px;
    }

    .section-odd {
      padding: 3em 0 4em;
      color: #247ba0;
      background-color: #fff;
    }

    .section-even {
      padding: 3em 0 4em;
      background-color: #247ba0;
      color: #fff;
    }

    .paragraph-annotation {
      color: #b2dbbf;
    }

    .ui.inverted.link.list .item, .ui.inverted.link.list a.item, .ui.inverted.link.list .item a:not(.ui) {
      color: rgba(255, 255, 255, 0.5);
    }

    .masthead {
      position: relative;
      color: #fff;
      min-height: 700px;
      padding: 1em 0em;
      background-position: right center;
      background-repeat: no-repeat;
      background-image: url('//az388900.vo.msecnd.net/photos/ehden_ny2015.jpg');
      background-size: cover;
    }

      .masthead .banner-call-to-action {
        position: relative;
      }

    .ui.curtain {
      position: absolute;
      top: 0;
      left: 0;
      right: 0;
      bottom: 0;
      background-color: rgba(0,0,0,0.5);
    }

    h1.ui.header {
      font-size: 3em;
      font-weight: normal;
    }

      h1.ui.header.banner {
        margin-top: 2em;
        margin-bottom: 0;
      }

    .row {
      margin-bottom: 3em;
    }

    .section-flat {
      display: none;
    }

    .footer.segment {
      text-align: left;
      color: #fff;
      background-color: #1B1C1D;
      padding: 5em;
    }

      .footer.segment a {
        display: block;
      }

    @@media only screen and (min-width: 500px) {
      .footer-column {
        position: relative;
        display: inline-block;
        width: 25%;
        padding-left: 1rem;
        padding-right: 1rem;
        vertical-align: top;
      }
    }

    @@media only screen and (min-width: 700px) {
      .section-flat {
        display: block;
      }

      .section-carousel {
        display: none;
      }

      .ui.list .list > .item, .ui.list > .item, ol.ui.list li, ul.ui.list li {
        display: list-item;
        table-layout: fixed;
        list-style-type: none;
        list-style-position: outside;
        padding: .21428571em 0;
        line-height: 1.14285714em;
      }

        .ui.list > .item:after, .ui.list > .list > .item, ol.ui.list > li:first-child:after, ul.ui.list > li:first-child:after {
          content: '';
          display: block;
          height: 0;
          clear: both;
          visibility: hidden;
        }

      .insta-post {
        width: 33%;
        display: inline-block;
      }

      .footer-column {
        width: 18%;
      }
    }

    @@media only screen and (min-width: 1200px) {
      .masthead h1.ui.header {
        margin-top: 3em;
        font-size: 4em;
      }
    }
  </style>
}

